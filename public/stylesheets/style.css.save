body {
  padding: 50px;
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
}

ul {
  list-style-type: none;
  margin: 0 !important;
  padding: 0 !important;
  overflow: hidden;
  background-color: #007BFF;
  width: 100% !important;
}

li {
  float: left;
}

li a {
  display: block;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

li a:hover {
  background-color: #111;
}

div.entryMain{
  display: block;
  color: white;}
  text-align: center;

a {
  padding: 14px 16px; color: #00B7FF;
  text-decoration: none;
}
